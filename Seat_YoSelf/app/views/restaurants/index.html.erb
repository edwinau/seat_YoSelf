<%@restaurants.each do |restaurant| %>
  <div class="each_rest">
    <p class="rest_name"> <%= restaurant.name%></p>
    <p class="rest_description"><%= restaurant.description%></p>
  </div>
<%end%>

<br>

<%if current_user%>
  <div id="new_rest_button"><%=link_to 'Create new restaurant', new_restaurant_path %></div>
<%end%>
